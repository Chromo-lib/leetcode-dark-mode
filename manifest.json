{
  "name": "WeatherNow",
  "version": "0.0.1",
  "description": "Get the Weather of your city",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "WeatherNow",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
}